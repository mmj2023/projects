import QtQuick 2.15
import QtQuick.Controls 2.15
import QtQuick.Layouts 1.15

ApplicationWindow {
    id: root
    visible: true
    width: 1920
    height: 30
    color: "#222"

    RowLayout {
        anchors.fill: parent
        spacing: 20

        Label {
            text: "Hello from plain QML"
            color: "white"
        }

        Label {
            text: Qt.formatDateTime(new Date(), "hh:mm:ss")
            color: "white"
        }
    }
}
